{
    "name": "php-ddd/domain-driven-design-bundle",
    "description": "DomainDrivenDesign integration for your Symfony2 project",
    "minimum-stability": "stable",
    "license": "MIT",
    "autoload": {
        "psr-4": {"PhpDDD\\DomainDrivenDesignBundle\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"PhpDDD\\DomainDrivenDesignBundle\\Tests\\": "tests"}
    },
    "authors": [
        {
            "name": "Julien Dufresne"
        }
    ],
    "require": {
        "php": ">=5.5",
        "php-ddd/domain-driven-design": "~0.6"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.8.1",
        "phpmd/phpmd": "^2.2.3",
        "phpunit/phpunit": "^4.7.0",
        "phploc/phploc": "^2.1.3",
        "satooshi/php-coveralls": "dev-master"
    }
}
